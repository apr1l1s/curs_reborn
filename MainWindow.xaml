<Window x:Class="curs_reborn.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:curs_reborn"
        mc:Ignorable="d"
        Title="Генератор степендионных ведомостей (Гость)"
        Height="500"
        Width="900"
        Background="White"
        Icon="Icons/icon.jpg" WindowStartupLocation="Manual" Cursor="Cross">
    <Grid>
        <Image
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Height="90"
            Grid.Row="0"
            Source="Icons/image.jpg" 
            Visibility="Visible"
            Margin="15"
        />
        <Frame 
            BorderBrush="#FF293A67"
            BorderThickness="2"
            x:Name="frame"
            NavigationUIVisibility="Hidden"
            Margin="180,10,10,10"
            Width="600"
            Height="400"
        />
    </Grid>
</Window>